# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'EthereumKit' do
  project 'EthereumKit.xcodeproj'
  workspace 'EthereumKit.xcworkspace'
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for EthereumKit

  target 'EthereumKitTests' do
    inherit! :search_paths
    # Pods for testing
  end

  pod 'CryptoSwift', '0.14.0'

end

target 'CryptoEthereumSwift' do
  project 'CryptoEthereumSwift.xcodeproj'
  workspace 'EthereumKit.xcworkspace'
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for EthereumKit

  pod 'CryptoSwift', '0.14.0'

end

target 'EthereumKitExample' do
  project 'EthereumKit.xcodeproj'
  workspace 'EthereumKit.xcworkspace'
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for EthereumKitExample
